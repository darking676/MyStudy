<script type="text/javascript">
	//window.a 윈도우 객체를 항상 갖고 있다
	func01();
	
	function func01(){
		console.log('func01 run');
	}
	//var func02=function (){console.log('func02 run')};
	(function (){console.log('func02 run');})();
	
	function func02(a,b){
		console.log(a,b);
	}
	func02(100);
	
	function func03(){
		console.log(arguments.length);
	}
	func03(10,20,30,40);
</script>
